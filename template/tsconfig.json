{
    "compilerOptions": {
      "target": "es6",    
      "lib": ["dom", "es2017"],       
      "module": "commonjs",
      "moduleResolution": "node",        
      "outDir": "./.temp",           
      "rootDir": "./",                     
      "esModuleInterop": true,     
      "skipLibCheck": true,
      "paths": {
        "@Step/*": ["./pickle-dev/step/*"],
        "@Actions": ["./pickle-dev/actions/index.ts"],
        "@PickleDecs": ["./PickleDecs.ts"],
        "@Alias": ["./alias"]
      }   
    },
}
